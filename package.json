{
	"name": "vivid-nx",
	"private": true,
	"version": "0.0.0",
	"license": "MIT",
	"repository": "github:Vonage/vivid-3",
	"scripts": {
		"start": "turbo run dev --filter=@repo/docs --filter=@vonage/vivid --filter=@repo/styles",
		"build": "turbo run build",
		"test": "turbo run test:coverage && istanbul-merge ./libs/*/coverage/coverage-final.json ./apps/*/coverage/coverage-final.json --out /dev/stdout | istanbul report --include /dev/stdin --dir coverage html lcov",
		"test:ui": "npx turbo run @vonage/vivid#e2e:docker:ui",
		"lint": "turbo run lint stylelint",
		"lint:fix": "turbo run lint:fix stylelint:fix",
		"typecheck": "turbo run typecheck",
		"format": "prettier --write .",
		"format:check": "prettier --check .",
		"postinstall": "patch-package"
	},
	"devDependencies": {
		"@typescript-eslint/eslint-plugin": "7.18.0",
		"vite": "5.4.15",
		"vite-plugin-dts": "~2.3.0",
		"@typescript-eslint/parser": "7.18.0",
		"@typescript-eslint/utils": "7.18.0",
		"@types/eslint": "^8.56.12",
		"@vitest/coverage-v8": "^2.1.9",
		"@vitest/ui": "^2.1.9",
		"eslint": "8.57.1",
		"eslint-config-prettier": "9.1.2",
		"eslint-plugin-ban": "^1.6.0",
		"eslint-plugin-compat": "^4.0.2",
		"eslint-plugin-import": "^2.32.0",
		"eslint-plugin-tsdoc": "^0.4.0",
		"patch-package": "^8.0.1",
		"eslint-plugin-unused-imports": "^3.2.0",
		"eslint-plugin-vue": "^9.33.0",
		"istanbul": "^0.4.5",
		"istanbul-merge": "^2.0.0",
		"prettier": "2.8.8",
		"stylelint": "^16.22.0",
		"turbo": "^2.5.5",
		"typescript": "5.9.3"
	},
	"overrides": {
		"minimist": "1.2.8",
		"path-to-regexp": "8.2.0",
		"express": {
			"path-to-regexp": "0.2.5"
		},
		"nconf": "0.13.0",
		"rollup": "^4.22.4",
		"braces": "3.0.3",
		"@11ty/eleventy-plugin-vite": {
			"vite": "5.4.15"
		},
		"happy-dom": "^20.0.2"
	},
	"workspaces": [
		"apps/*",
		"libs/*",
		"turbo/generators"
	],
	"packageManager": "npm@10.9.3+sha512.e84875bb943e908557780f1eee5d9cfc7a67145730ae4b77ef10ccba30f96ded6096859af69ea3dc5b2fde60725d79aa247cbed9c12544c30bf28a4d4fbc4825"
}
