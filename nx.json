{
	"npmScope": "vivid-nx",
	"affected": {
		"defaultBase": "main"
	},
	"implicitDependencies": {
		"package.json": {
			"dependencies": "*",
			"devDependencies": "*"
		},
		".eslintrc.json": "*",
		".stylelintrc.json": "*",
		"tools/eslint-rules/**/*": "*"
	},
	"tasksRunnerOptions": {
		"default": {
			"runner": "nx/tasks-runners/default",
			"options": {
				"cacheableOperations": ["build", "lint", "test", "e2e", "stylelint"]
			}
		}
	},
	"generators": {
		"@nrwl/web:application": {
			"style": "scss",
			"linter": "eslint",
			"unitTestRunner": "jest",
			"e2eTestRunner": "cypress"
		},
		"@nrwl/web:library": {
			"style": "scss",
			"linter": "eslint",
			"unitTestRunner": "jest"
		}
	},
	"$schema": "./node_modules/nx/schemas/nx-schema.json",
	"targetDefaults": {
		"build": {
			"dependsOn": ["^build"]
		}
	}
}
