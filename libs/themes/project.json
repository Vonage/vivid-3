{
  "root": "libs/themes",
  "sourceRoot": "libs/themes/src",
  "projectType": "library",
  "targets": {
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/libs/themes"],
      "options": {
        "jestConfig": "libs/themes/jest.config.js",
        "passWithNoTests": true
      }
    },
    "build": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "commands": [
          "sass libs/themes/src/lib/theme:dist/libs/themes/theme",
          "sass libs/themes/src/lib/fonts:dist/libs/themes/fonts"
        ]
      }
    },
    "serve": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "command": "sass libs/themes/src/lib/theme:dist/libs/themes/theme --watch"
      }
    },
    "stylelint": {
      "executor": "nx-stylelint:lint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["libs/themes/**/*.css"]
      }
    }
  },
  "tags": []
}
