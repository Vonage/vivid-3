{
	"extends": ["../../.eslintrc.json", "prettier"],
	"ignorePatterns": ["!**/*"],
	"rules": {
		"@typescript-eslint/no-non-null-assertion": "off",
		"@typescript-eslint/no-attribute-default-value": "off"
	},
	"overrides": [
		{
			"files": ["*.ts", "*.tsx", "*.js", "*.jsx"],
			"rules": {}
		},
		{
			"files": ["*.ts", "*.tsx"],
			"rules": {}
		},
		{
			"files": ["*.js", "*.jsx"],
			"rules": {}
		},
		{
			"files": ["*.json"],
			"parser": "jsonc-eslint-parser",
			"rules": {}
		}
	]
}
