{
  "$schema": "https://developer.microsoft.com/json-schemas/api-extractor/v7/api-extractor.schema.json",
  "mainEntryPointFilePath": "./../../dist/libs/components/index.d.ts",
  "docModel": {
    "enabled": true,
    "apiJsonFilePath": "./../../dist/libs/components/<unscopedPackageName>.api.json"
  },
  "dtsRollup": {
    "enabled": false
  },
  "apiReport": {
    "enabled": false
  },
  "messages": {
    "extractorMessageReporting": {
      "ae-missing-release-tag": {
        "logLevel": "none"
      },
      "ae-forgotten-export": {
        "logLevel": "none"
      }
    }
  }
}