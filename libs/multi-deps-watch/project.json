{
	"name": "multi-deps-watch",
	"$schema": "../../node_modules/nx/schemas/project-schema.json",
	"sourceRoot": "libs/multi-deps-watch/src",
	"projectType": "library",
	"targets": {
		"build": {
			"executor": "@nx/vite:build",
			"outputs": ["{options.outputPath}"],
			"options": {
				"outputPath": "dist/libs/multi-deps-watch"
			}
		},
		"publish": {
			"command": "node tools/scripts/publish.mjs multi-deps-watch {args.ver} {args.tag}",
			"dependsOn": ["build"]
		},
		"test": {
			"executor": "@nx/vite:test",
			"outputs": ["{options.reportsDirectory}"],
			"options": {
				"passWithNoTests": true,
				"reportsDirectory": "../../coverage/libs/multi-deps-watch"
			}
		},
		"lint": {
			"executor": "@nx/eslint:lint",
			"outputs": ["{options.outputFile}"],
			"options": {
				"lintFilePatterns": [
					"libs/multi-deps-watch/**/*.ts",
					"libs/multi-deps-watch/package.json"
				]
			}
		}
	},
	"tags": []
}
