{
  "projectType": "application",
  "root": "apps/vivid-demo",
  "sourceRoot": "apps/vivid-demo/src",
  "tags": [],
  "targets": {
    "build": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "pathprefix": "",
        "commands": [
          {
            "command": "npm run nx build components && npm run eleventy --config=./apps/docs/.eleventy.js"
          }
        ]
      },
      "configurations": {
        "production": {
          "pathprefix": "vivid-3"
        }
      }
    },
    "serve": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "commands": [
          {
            "command": "yarn eleventy --config=./apps/docs/.eleventy.js --serve"
          }
        ]
      }
    }
  }
}
