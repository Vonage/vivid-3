{
  "projectType": "application",
  "root": "apps/vivid-demo",
  "sourceRoot": "apps/vivid-demo/src",
  "tags": [],
  "targets": {
    "build": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "pathprefix": "",
        "commands": [
          {
            "command": "npx eleventy --config=./apps/docs/.eleventy.js"
          }
        ]
      }
    },
    "serve": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "command": "npx eleventy --config=./apps/docs/.eleventy.js --serve & npx nx serve styles & npx nx build components --watch"
      }
    }
  },
  "implicitDependencies": [
    "components",
		"styles"
  ]
}
