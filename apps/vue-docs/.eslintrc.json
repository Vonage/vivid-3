{
  "rules": {
    "vue/multi-word-component-names": 0,
    "vue/no-multiple-template-root": 0,
    "vue/component-name-in-template-casing": ["error", "PascalCase" , {
      "registeredComponentsOnly": false,
      "ignores": []
    }],
    "vue/html-self-closing": ["error", {
      "html": {
        "void": "always",
        "normal": "always",
        "component": "always"
      },
      "svg": "always",
      "math": "always"
    }]
  }
}
