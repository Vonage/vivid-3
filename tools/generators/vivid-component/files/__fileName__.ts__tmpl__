import { FoundationElement } from '@microsoft/fast-foundation';
import { attr } from '@microsoft/fast-element';


/**
 * Base class for <%= name %>
 *
 * @public
 */
export class <%= className %> extends FoundationElement {

	/**
	 * Indicates the <%= name %>'s text.
	 *
	 * @public
	 * @remarks
	 * HTML Attribute: text
	 */
	@attr text = '';
}
